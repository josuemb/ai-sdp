{
  "name": "ai-sdp",
  "description": "AI-Assisted Software Development Process - Guides through structured development cycles with web research, collaborative decision-making, state management, and comprehensive project tracking",
  "prompt": "file://./ai-sdp/prompt.md",
  "mcpServers": {
    "aws-knowledge-mcp-server": {
      "url": "https://knowledge-mcp.global.api.aws",
      "type": "http",
      "disabled": false,
      "autoApprove": [
        "search_documentation",
        "read_documentation",
        "recommend",
        "list_regions",
        "get_regional_availability"
      ]
    },
    "awslabs.aws-diagram-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-diagram-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "autoApprove": []
    }
  },
  "tools": [
    "fs_read",
    "fs_write",
    "execute_bash",
    "web_search",
    "web_fetch",
    "introspect",
    "code",
    "@aws-knowledge-mcp-server",
    "@awslabs.aws-diagram-mcp-server"
  ],
  "allowedTools": [
    "fs_read",
    "fs_write",
    "execute_bash",
    "web_search",
    "web_fetch",
    "introspect",
    "code",
    "@aws-knowledge-mcp-server",
    "@awslabs.aws-diagram-mcp-server"
  ]
}
